#!/bin/bash
pass=$1
serverSql=172.18.2.1
base=tools
user=tools
basepass=tools
if [ ! -z "$pass" ]; then
		DIR="/opt/WWW/container.ite-ng.ru/projects/tools"
		if [ ! -d "$DIR" ]; then
			mkdir -p $DIR
			tar -zxf /tmp/kzkt45.sql.tar.gz -C /tmp
			chown apache:apache -R $DIR
			cd $DIR
			init.sql $pass $serverSql $base $user $basepass
		fi
fi
