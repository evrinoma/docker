#!/bin/bash
GITLAB="/var/opt/gitlab"
GIT="/opt/WWW/container.ite-ng.ru/data/gitlab"
if [ ! -d "$GIT" ]; then
	mkdir -p $GIT
	mv $GITLAB/*  $GITLAB/.[^.]* $DIR/
fi
